
@{
    ViewBag.Title = "Admin Dashboard";
}

<h2>Welcome : @ViewBag.Name</h2>
<hr>
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>                
            </button>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Registered Users", "ViewUsers", new { controller = "Register", @UserId = ViewBag.UserId })</li>
                <li>@Html.ActionLink("Registered Vendors", "ViewVendors", new { controller = "Register", @UserId = ViewBag.UserId })</li>
                <li>@Html.ActionLink("Bookings", "Index", new { controller = "Booking", @UserId = ViewBag.UserId })</li>
                <li>@Html.ActionLink("Vehicles", "Index", new { controller = "Vehicle", @UserId = ViewBag.UserId })</li>
                <li style="padding-left: 250px;">@Html.ActionLink("Logout", "SignOut", new { Controller = "Login" })</li>
            </ul>
        </div>
    </div>
</div>


